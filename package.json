{
	"name": "mag",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@types/jest": "24.0.18",
		"@types/node": "12.7.5",
		"@types/preval.macro": "^3.0.0",
		"@types/react": "16.9.2",
		"@types/react-dom": "16.9.0",
		"@types/react-router-dom": "^5.1.0",
		"escape-string-regexp": "^2.0.0",
		"idb": "^4.0.4",
		"preval.macro": "^3.0.0",
		"qs": "^6.9.0",
		"react": "^16.9.0",
		"react-dom": "^16.9.0",
		"react-router-dom": "^5.1.0",
		"react-scripts": "3.2.0",
		"typescript": "3.7.2",
		"use-memo-one": "^1.1.1"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": "react-app",
		"rules": {
			"no-restricted-imports": [
				"error",
				{
					"paths": [
						{
							"name": "react",
							"importNames": [
								"useMemo",
								"useCallback"
							],
							"message": "`useMemo` and `useCallback` are subject to cache busting. Please use `useMemoOne`"
						},
						{
							"name": "use-memo-one",
							"importNames": [
								"useMemoOne",
								"useCallbackOne"
							],
							"message": "use-memo-one exports `useMemo` and `useCallback` which work nicer with `eslint-plugin-react-hooks`"
						}
					]
				}
			],
			"no-restricted-properties": [
				2,
				{
					"object": "React",
					"property": "useMemo",
					"message": "Please use useMemo from 'use-memo-one'."
				},
				{
					"object": "React",
					"property": "useCallback",
					"message": "Please use useCallback from 'use-memo-one'."
				}
			]
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@types/qs": "^6.5.3"
	}
}
